{
    "name": "r4j4h/jspackager-test",
    "description": "Annotation based dependency resolution system for front end file inclusion.",
    "keywords": ["dependency resolution", "file inclusion", "compilation"],
    "license": "MIT",
    "require": {
        "psr/log": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "phing/phing": "2.*",
        "phpmd/phpmd": "2.1.1",
        "phploc/phploc": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "JsPackager\\": "src/JsPackager/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsPackager\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev",
            "dev-master": "1.0.x-dev"
        }
    }
}
