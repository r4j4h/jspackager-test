{
    "name": "r4j4h/jspackager-test",
    "description": "Annotation based dependency resolution system for front end file inclusion.",
    "keywords": ["dependency resolution", "file inclusion", "compilation"],
    "license": "MIT",
    "require": {
        "psr/log": "1.0.0",
        "fzaninotto/Streamer": "0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.7",
        "phing/phing": "2.11.x",
        "phpmd/phpmd": "2.2.x",
        "phploc/phploc": "2.1.x",
        "mikey179/vfsStream": "^1.5",
        "behat/behat": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "JsPackager\\": "src/JsPackager/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JsPackager\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.*-dev",
            "dev-master": "2.0.*-dev"
        }
    }
}
